<h1>Listing teachings</h1>

<table>
  <tr>
    <th>Instructor id</th>
    <th>Course id</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teachings.each do |teaching| %>
  <tr>
    <td><%= teaching.Instructor_ID %></td>
    <td><%= teaching.Course_ID %></td>
    <td><%= link_to 'Show', teaching %></td>
    <td><%= link_to 'Edit', edit_teaching_path(teaching) %></td>
    <td><%= link_to 'Destroy', teaching, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

